{
  "version": 2,
  "name": "your-project-name",
  "builds": [
    { "src": "frontend/**/*", "use": "@vercel/static-build" },
    { "src": "backend/**/*", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/backend/$1" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
